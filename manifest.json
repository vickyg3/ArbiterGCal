{
  "manifest_version": 2,
  "name": "Arbiter Schedule to Google Calendar",
  "version": "1.0",
  "description": "Add games from Arbiter to Google Calendar.",
  "permissions": ["http://*.arbitersports.com/", "https://*.arbitersports.com/"],
  "content_scripts": [
    {
      "matches": [
        "http://*.arbitersports.com/*",
      	"https://*.arbitersports.com/*"
      ],
      "js": ["jquery.min.js", "content_script.js"]
    }
  ]
}
